@model NextMile02.Models.FacebookUserModel

@{
    ViewBag.Title = "UserDetails";
    
}

<div id="partialPlaceHolder">
    <div class="container target glibc jumbotron" style="background-color: rgba(0,0,0,0.5); color: whitesmoke;">
        <div class="row">
            <div class="col-sm-12">
                <div class="col-sm-5">
                    <h2 class="pull-left">Welcome! @Model.name</h2>
                    <button type="button" class="btn btn-success top">View Preferences</button>
                    <button type="button" class="btn btn-success top" data-target="#myModal">Contact Us</button>
                </div>

                <a href="/users" class="pull-right"><img title="profile image" class="img-circle img-responsive" src='@Model.picture.data.url'></a>
            </div>
        </div>

    </div>

</div>
@*Javascript calls*@
<script src="~/Scripts/javascript/jquery-1.10.2.js"></script>
<script src="//ajax.googleapis.com/ajax/libs/jquery/2.0.0/jquery.min.js"></script>



